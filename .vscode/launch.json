  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/truffle-config.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "name": "truffle exec",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/truffle/build/cli.bundled.js",
      "args": ["exec", "order-test.js"],
      "cwd": "${workspaceRoot}",
      "outFiles": [
          "${workspaceRoot}/**/*.js"
      ],
    }
  ]
}